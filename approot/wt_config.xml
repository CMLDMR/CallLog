<server>
  <application-settings location="*">
    <properties>
      <property name="leafletJSURL">https://unpkg.com/leaflet@1.5.1/dist/leaflet.js</property>
      <property name="leafletCSSURL">https://unpkg.com/leaflet@1.5.1/dist/leaflet.css</property>
    </properties>
    <log-config>* -info -debug</log-config>
    <max-request-size>10240</max-request-size>
    <session-management>
      <server-push-timeout>5</server-push-timeout>
    </session-management>
  </application-settings>
</server>
